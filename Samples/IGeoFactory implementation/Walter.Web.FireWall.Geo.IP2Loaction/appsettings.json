{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Walter": "Error"
    }
  },
  //firewall storage database, use a connection string that makes sense in your hosting, the bellow string uses a localdb instance that is not supported with most ISP's
  "ConnectionStrings": {
    "fireWallStateIP2Location": "Server=(localdb)\\mssqllocaldb;Initial Catalog=fireWallStateIP2Location;Trusted_Connection=Yes;"
  },
  "AllowedHosts": "*"
}
